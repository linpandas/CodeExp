   "source": [
    "m, n = 3, 7\n",
    "\n",
    "dp = [[0]*n for _ in range(m)]\n",
    "\n",
    "dp[0][0] = 0\n",
    "\n",
    "for i in range(1, m):\n",
    "    dp[i][0] = 1\n",
    "\n",
    "for j in range(1, n):\n",
    "    dp[0][j] = 1\n",
    "\n",
    "for i in range(1, m):\n",
    "    for j in range(1, n):\n",
    "        dp[i][j] = dp[i-1][j] + dp[i][j-1]\n",
    "\n",
    "dp"
   ]
