   "source": [
    "n = 1\n",
    "\n",
    "dp = [float(\"inf\")] * (n+1) # 和为j的完全平方数的最小数量为dp[j]\n",
    "dp[0] = 0\n",
    "\n",
    "for i in range(1, int(n ** 0.5) + 1):\n",
    "    for j in range(i*i, n+1):\n",
    "        print(dp)\n",
    "        dp[j] = min(dp[j - i*i] + 1, dp[j])\n",
    "\n",
    "dp"
   ]
