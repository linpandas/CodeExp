    "s = \"abcabcbb\"\n",
    "\n",
    "l = 0\n",
    "count = 0\n",
    "from collections import deque\n",
    "queue = deque()\n",
    "for r in range(len(s)):\n",
    "    if s[r] in queue:\n",
    "        count = max(count, len(queue))\n",
    "        while s[r] in queue:\n",
    "            queue.popleft()\n",
    "    queue.append(s[r])\n",
    "count"
   ]
